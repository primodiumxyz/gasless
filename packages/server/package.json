{
  "name": "@primodiumxyz/gasless-server",
  "version": "1.0.0",
  "description": "A library for creating a MUD-compliant gasless server.",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "bin": {
    "gasless-server": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "NODE_ENV=development dotenvx run -- tsx watch src/index.ts",
    "start": "NODE_ENV=production tsx src/index.ts",
    "test": "NODE_ENV=test dotenvx run -f .env.test -- vitest --run",
    "test:ui": "NODE_ENV=test dotenvx run -f .env.test -- vitest --ui",
    "test:watch": "NODE_ENV=test dotenvx run -f .env.test -- vitest"
  },
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/session": "^10.9.0",
    "@latticexyz/world-modules": "^2.0.12",
    "connect-redis": "^7.1.1",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.0.0",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "viem": "^2.9.20"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@latticexyz/common": "^2.0.12",
    "@latticexyz/protocol-parser": "^2.0.12",
    "@latticexyz/store": "2.0.9",
    "@latticexyz/world": "2.0.9",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@vitest/ui": "^2.0.1",
    "abitype": "^1.0.5",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.1"
  }
}
